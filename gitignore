# Ignore macOS system files that might pollute your project
.DS_Store

# Ignore editor-specific files (example for Visual Studio Code)
.vscode/

# Ignore npm package manager directory
node_modules/

# Ignore yarn package manager directory
/yarn-error.log
.yarn/

# Ignore log files
*.log

# Ignore temporary files
*.tmp

# Ignore IDE files (e.g., JetBrains IDEs)
.idea/
*.iml

# Ignore compiled binary files
*.class
*.dll
*.exe
*.o
*.obj

# Ignore image thumbnails generated by operating systems
Thumbs.db
ehthumbs.db

# Ignore environment-specific files
.env
.env.local
.env.*.local
.env.test
.env.production

# Ignore dependency directories
/bower_components/
/jspm_packages/

# Ignore build output directories
/dist/
/build/
/out/

# Ignore cache files
*.cache

# Ignore any file named .gitignore itself
.gitignore

# Ignore system-specific or hidden files
/.DS_Store
/.idea/
/.vscode/
/.npmrc

# Ignore backup files created by text editors or version control systems
*~

# Ignore macOS folder attributes
._*

# Ignore SQLite database files
*.sqlite
*.sqlite3

# Ignore Python bytecode files
__pycache__/

# Ignore compiled Python files
*.pyc
*.pyo

# Ignore Node.js debug log
node_debug.log

# Ignore npm debug log
npm-debug.log
